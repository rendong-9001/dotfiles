  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed",
      "listen": "0.0.0.0",
      "listen_port": 14443
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "vless",
      "tag": "vless-out",
      "connect_timeout": "10s",
      "tcp_fast_open": true,
      "domain_resolver": "txdns",
      "server": "example.com",
      "server_port": 443,
      "uuid": "uuid",
      "network": "tcp",
      "tls": {
        "enabled": true,
        "server_name": "example.com",
        "min_version": "1.2"
      },
      "multiplex": {
        "enabled": true,
        "protocol": "smux",
        "max_connections": 1,
        "min_streams": 4,
        "padding": true,
        "brutal": {
          "enabled": true,
          "up_mbps": 50,
          "down_mbps": 100
        }
      },
      "transport": {
        "type": "ws",
        "path": "/watch",
        "headers": {
          "Accept-Language": "en-US,en;q=0.5",
          "Host": "example.com",
          "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:144.0) Gecko/20100101 Firefox/144.0"
        },
        "max_early_data": 2080,
        "early_data_header_name": "Sec-WebSocket-Protocol"
      }
    },
    {
      "type": "tuic",
      "tag": "tuic-out",
      "connect_timeout": "10s",
      "domain_resolver": "txdns",
      "server": "example.com",
      "server_port": 443,
      "uuid": "uuid",
      "password": "password",
      "congestion_control": "bbr",
      "udp_relay_mode": "quic",
      "heartbeat": "10s",
      "network": "tcp",
      "tls": {
        "enabled": true,
        "server_name": "example.com",
        "alpn": "h3"
      }
    }
  ],
