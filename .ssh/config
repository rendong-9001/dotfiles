Host github.com
	Port 443
	HostName ssh.github.com
	User git
	IdentityFile ~/.ssh/xxx
	IdentitiesOnly yes
	# ProxyCommand nc -X 5 -x 127.0.0.1:13131 %h %p

Host codeberg.org
	Port 22
	HostName codeberg.org
	User git
	IdentityFile ~/.ssh/xxx
	IdentitiesOnly yes
	# ProxyCommand nc -X 5 -x 127.0.0.1:13131 %h %p

Host sshjump
	Port 22
	Hostname xxx
	User xxx 
	IdentityFile ~/.ssh/xxx
	IdentitiesOnly yes
	ServerAliveCountMax 3
	ServerAliveInterval 30
	TCPKeepAlive yes
	ConnectTimeout 10
	ConnectionAttempts 2
	ExitOnForwardFailure yes
	ForwardAgent no
	StrictHostKeyChecking yes
	LogLevel ERROR

Host custom
	Port 22
	HostName xxx
	User xxx
	IdentityFile ~/.ssh/xxx
	IdentitiesOnly yes
	ServerAliveCountMax 3
	ServerAliveInterval 30
	TCPKeepAlive yes
	ConnectTimeout 10
	ConnectionAttempts 2
	ControlMaster auto
	ControlPersist 10m
	ControlPath ~/.ssh/control/control-%h-%p-%r
	ExitOnForwardFailure yes
	StrictHostKeyChecking accept-new
	ProxyJump sshjump
	LogLevel ERROR
	# ProxyCommand nc -X 5 -x 127.0.0.1:13131 %h %p
