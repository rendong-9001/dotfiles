Host custom
	Port 22
	HostName xxx
	User xxx
	IdentityFile ~/.ssh/xxx
	IdentitiesOnly yes
	ServerAliveCountMax 4
	ServerAliveInterval 60
	TCPKeepAlive yes

Host github
	Port 443
	HostName ssh.github.com
	User git
	IdentityFile ~/.ssh/xxx
	IdentitiesOnly yes
	#ProxyCommand nc -X 5 -x 127.0.0.1:13131 %h %p
	#AddKeysToAgent yes


